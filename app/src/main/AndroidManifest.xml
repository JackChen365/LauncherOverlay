<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cz.launcher.overlay.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".dialog.DualScreenSampleActivity"></activity>
        <activity android:name=".tab.TabSampleActivity" />
        <activity android:name=".search.SearchSampleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fixed.NestedWindowSampleActivity" />

        <service
            android:name=".fixed.service.LauncherOverlayService"
            android:enabled="true" />
        <service
            android:name=".search.service.LauncherSearchService"
            android:enabled="true" />
    </application>

</manifest>